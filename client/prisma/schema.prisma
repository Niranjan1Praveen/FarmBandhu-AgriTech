generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model MandiLatLong {
  id        Int    @id @default(autoincrement())
  State     String
  Mandi     String
  Latitude  Float
  Longitude Float
}
